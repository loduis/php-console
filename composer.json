{
    "name": "loduis/php-console",
    "authors": [
        {
            "name": "Loduis Madariaga Barrios",
            "email": "loduis@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/finder": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "PhpConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpConsole\\Tests\\App\\": "tests/app/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra" : {
        "branch-alias" : {"dev-master" : "6.0.x-dev"}
    }
}
